(symbols 'printer 'pico)

(de to_hex (@PADDING "VALUE")
  (default @PADDING 0)
  (curry (@PADDING) ("VALUE")
    (when (num? "VALUE")
      (pad @PADDING (hex "VALUE"))
      )))

(de to_dec (@POINT "VALUE")
  (default @POINT 0)
  (curry (@POINT) ("VALUE")
    (when (num? "VALUE")
      (format "VALUE" @POINT)
      )))
